                </svg>
                <span style="padding-left: 5px;">
                  Employés<span class="badge bg-primary poppins" style="margin-left: 5px;">{{
                      app.session.get('employees_count')
                    }}</span>
                </span>
              </a>
            </li>
            <li class="nav-item">
              <a class="nav-link poppins regular"
                href="table.html"
                style="padding: 10px;color: rgb(33,37,41);">
